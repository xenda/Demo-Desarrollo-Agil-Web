
<label class="label"><%= t("activerecord.attributes.project.name", :default => t("activerecord.labels.name", :default => "Name")) %>:</label>
<p><%= @project.name %></p>

<div class="form-actions">
<%= link_to "Back", projects_path, :class => 'btn'  %>
<%= link_to "Edit", edit_project_path(@project), :class => 'btn' %>
<%= link_to "Delete", project_path(@project), :method => "delete", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}", :class => 'btn' %>
</div>
